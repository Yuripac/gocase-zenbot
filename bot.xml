<context>
  <input id="purchase" pattern="* blablableble *">

    <var name="RequestedOrder" value="javascript: $req_text.match(/\d{13}/) ? $req_text.match(/\d{13}/)[0] : null"/>
    <get url="http://dev.gocase.com.br:3000/api/v1/zenbot/order_details" var="Data">
      <param name="email" value="nexiste@gocase.com.br"/>
      <param name="reference" value="$RequestedOrder"/>
    </get>

    <var name="OrderNumber" value="javascript: $Data.order.number"/>
    
    <output value="$OrderNumber"/>
    
  </input>
</context>